# === Portreva v3.3 â€” .env.example ===
# Public base (reverse proxy must map to this)
WEBHOOK_URL=https://portreva.example.com/
N8N_HOST=portreva.example.com
N8N_PROTOCOL=https

# n8n security
N8N_ENCRYPTION_KEY=CHANGE_ME_TO_LONG_RANDOM
N8N_BLOCK_ENV_ACCESS_IN_NODE=true

# Metrics (protect behind proxy auth if enabled)
N8N_METRICS=true
N8N_METRICS_INCLUDE_QUEUE_METRICS=true

# Limits (MiB)
N8N_PAYLOAD_SIZE_MAX=64
N8N_FORMDATA_FILE_SIZE_MAX=512

# Postgres
POSTGRES_USER=portreva
POSTGRES_PASSWORD=strong_password_here
POSTGRES_DB=portreva

# Worker
N8N_WORKER_CONCURRENCY=5

# Providers
REPLICATE_API_TOKEN=
REPLICATE_WEBHOOK_SECRET=  # whsec_... from Replicate
FAL_API_KEY=
# For fal.ai, public JWKS is fetched from their well-known URL; no shared secret needed.

# remove.bg
REMOVEBG_API_KEY=

# Cloudinary
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_UPLOAD_PRESET= # image upload preset (unsigned ok for dev, use signed in prod)
CLOUDINARY_UPLOAD_PRESET_RAW= # raw upload preset for ZIP

# Notifications (optional)
ERROR_NOTIFY_WEBHOOK=
